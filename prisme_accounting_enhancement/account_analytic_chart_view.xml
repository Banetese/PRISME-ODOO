<?xml version="1.0"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="prisme_account_analytic_chart_wizard">
			<field name="name">account.analytic.chart.wizard.prisme</field>
			<field name="model">account.analytic.chart</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="account.account_analytic_chart_view" />
			<field name="arch" type="xml">
			    <xpath expr="/form/group" position="before">
					<group string="Select the Lines Filters" col="4">
						<field name="financial_account_filter" colspan="4"/>
					</group>					
			    </xpath>
			</field>
		</record>
	</data>
</openerp>